{
  "name": "remind-clone-web-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node ./bin/www",
    "devmon": "cross-env NODE_ENV=development nodemon ./bin/www",
    "debug": "cross-env DEBUG=* NODE_ENV=development nodemon ./bin/www",
    "lint:prettier": "prettier --write . --loglevel warn",
    "lint:eslint": "eslint --fix",
    "migrate:up": "knex migrate:latest --knexfile ./config/knexfile.js",
    "migrate:down": "knex migrate:down --knexfile ./config/knexfile.js",
    "seed:make": "knex seed:make --knexfile ./config/knexfile.js",
    "seed:run": "knex seed:run --knexfile ./config/knexfile.js",
    "start": "node ./bin/www",
    "test": "jest --forceExit"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "node-schedule": "^1.3.2",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^5.0.0"
  }
}
